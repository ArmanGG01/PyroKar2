{
  "name": "PyroKar2-Userbot",
  "description": "This app does one little thing, and does it well.",
  "keywords": [
    "productivity",
    "HTML5",
    "scalpel"
  ],
  "website": "https://small-sharp-tool.com/",
  "repository": "https://github.com/ArmanGG01/PyroKar2-Userbot",
  "logo": "https://telegra.ph//file/5f3929a7c65ed2dfd93db.jpg",
  "success_url": "/welcome",
  "scripts": {
    "postdeploy": "bundle exec rake bootstrap"
  },
   "env": {
   "STRING_SESSION1": {
   "description": "Untuk String Bisa Langsung Ambil Di @karstring_bot",
   "required": true
  },
   "MONGO_URL": {
   "description": "Ambil mongodb di mongodb.com Untuk Databasenya",
   "required": true
  },
   "SUDO_USERS": {
   "description": "Daftar Pengguna (Id Pengguna)",
   "required": true
  },
   "OWNER_ID": {
   "description": "Isi Dengan Id Akun Mu (Untuk Akses Semua Fiturnya)",
   "required": true
  },
   "BOT_TOKEN": {
    "description": "Pergilah Ke Telegram Lalu Ambil bot token Dari @BotFather Yang Berada Di Telegram",
    "required": true,
    "value": ""
     }
  },
  "formation": {
    "web": {
      "quantity": 1,
      "size": "standard-1x"
    }
  },
  "image": "heroku/ruby",
  "addons": [
    "openredis",
    {
      "plan": "mongolab:shared-single-small",
      "as": "MONGO"
    },
    {
      "plan": "heroku-postgresql",
      "options": {
        "version": "9.5"
      }
    }
  ],
  "buildpacks": [
    {
      "url": "https://github.com/stomita/heroku-buildpack-phantomjs"
    }
  ],
  "environments": {
    "test": {
      "scripts": {
        "test": "bundle exec rake test"
      }
    }
  }
}
